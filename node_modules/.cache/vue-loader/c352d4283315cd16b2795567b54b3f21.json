, nameProduct, priceProduct, photoProduct) {\n      var productStored = {\n        id: idProduct,\n        name: nameProduct,\n        price: priceProduct,\n        photo: photoProduct\n      };\n\n      this.keranjangUser.push(productStored);\n      const parsed = JSON.stringify(this.keranjangUser);\n      localStorage.setItem(\"keranjangUser\", parsed);\n\n      window.location.reload();\n    }\n  }\n}\n</script>\n<style scoped>\n.product-item {\n  margin-right: 25px;\n}\n.pi-pic img {\n  height: 450px;\n}\n</style>"]}]}